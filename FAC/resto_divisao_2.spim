.data
prompt: .asciiz "Digite um numero inteiro: \n"
result: .asciiz "O resto da divisao por 2 e: "

.text
.globl main

main:
    li $v0, 4              
    la $a0, prompt         
    syscall

    li $v0, 5             
    syscall
    move $t0, $v0         

    andi $t1, $t0, 1      

    li $v0, 4             
    la $a0, result        
    syscall

    li $v0, 1              
    move $a0, $t1          
    syscall

    li $v0, 10            
    syscall

